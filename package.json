{
	"name": "candy-trap-web-app",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"engines": {
		"node": ">=20.0.0",
		"npm": ">=10.0.0"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"deploy": "npm run build && npx gh-pages -d build",
